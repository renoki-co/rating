{
    "name": "rennokki/rating",
    "description": "Laravel Eloquent Rating allows you to assign ratings to any model.",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "rating",
        "rate",
        "stars",
        "star",
        "system"
    ],
    "homepage": "https://github.com/renoki-co/rating",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "alex@renoki.org",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "^7.30|^8.23|^9.0",
        "illuminate/support": "^7.30|^8.23|^9.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.5|^1.4.4",
        "orchestra/testbench": "^6.24.0|^7.0",
        "orchestra/database": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Rating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Rating\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Rating\\RatingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
